@model Project_Assesment.Models.Project

    <div class="form-group">
      <label class="control-label col-sm-2" for="email"> @Html.LabelFor(m=>m.title):</label>
      <div class="col-sm-10">
        @Html.EditorFor(m=>m.title)
          @Html.ValidationMessageFor(m=>m.title)
      </div>
    </div>
   <div class="form-group">
      <label class="control-label col-sm-2" for="email"> @Html.LabelFor(m=>m.description):</label>
      <div class="col-sm-10">
        @Html.EditorFor(m=>m.description)
          @Html.ValidationMessageFor(m=>m.description)
      </div>
    </div>
    <div class="form-group">
      <label class="control-label col-sm-2" for="email"> @Html.LabelFor(m=>m.start_date):</label>
      <div class="col-sm-10">
        @Html.TextBoxFor(m => m.start_date, new { @type="date"})
          @Html.ValidationMessageFor(m=>m.start_date)
      </div>
    </div>

       <div class="form-group">
      <label class="control-label col-sm-2" for="email"> @Html.LabelFor(m=>m.end_date):</label>
      <div class="col-sm-10">
        @Html.TextBoxFor(m => m.end_date, new { @type="date"})
          @Html.ValidationMessageFor(m=>m.end_date)
      </div>
    </div>
      <div class="form-group">
      <label class="control-label col-sm-2" for="email"> @Html.LabelFor(m=>m.end_date):</label>
      <div class="col-sm-10">
        @Html.TextBoxFor(m => m.end_date, new { @type="date"})
          @Html.ValidationMessageFor(m=>m.end_date)
      </div>
    </div>

    <div class="form-group">        
      <div class="col-sm-offset-2 col-sm-10">
        <button type="submit" class="btn btn-default">Submit</button>
      </div>
    </div>
  